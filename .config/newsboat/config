# general settings
# -----------
auto-reload yes
max-items 50 
reload-time 60 
reload-threads 100
prepopulate-query-feeds yes
cleanup-on-quit yes
text-width 80


# unbind and bind
# -----------
unbind-key ENTER
unbind-key j
unbind-key k
unbind-key J
unbind-key K

bind-key j down
bind-key k up
bind-key l open
bind-key h quit

bind-key H prev-feed
bind-key L next-feed
bind-key g home
bind-key G end

